#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    long long t;
    cin>>t;
    
   
  
    
    while(t--)
    { long long x,y,z,w;
          
    cin>>x>>y>>z>>w;
        
    
    
        cout<<(2*x)-z<<" ";
        cout<<(2*y)-w;
        cout<<endl;
    
    
    }
    return 0;

    }
    
